{
    "name": "rmcgirr83/topicicononindex",
    "type": "phpbb-extension",
    "description": "Displays topic icons on index page of forum",
    "version": "1.1.2",
    "time": "2020-09-12",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.2",
        "composer/installers": "~1.0.0"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Topic icon on index",
        "soft-require": {
            "phpbb/phpbb": ">=3.2"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/topic_icon_on_index",
            "filename": "version_check",
            "ssl": true
        }
    }
}