{
    "name": "senky/clipboardimage",
    "type": "phpbb-extension",
    "description": "Allows to paste image from clipboard to be added as an attachment.",
    "version": "2.0.2",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Bakasura",
            "email": "bakasura@protonmail.ch",
            "role": "Fork Developer"
        },
        {
            "name": "Jakub Senko",
            "email": "jakubsenko@gmail.com",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.0"
    },
    "extra": {
        "display-name": "Clipboard Image",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/clipboardimage",
            "filename": "version_check"
        }
    }
}